 from datetime import datetime
>>> now=datetime.now()
>>> year=now.strftime("%Y")
>>> print("year",year)

year 2019

>>> month=now.strftime("%m")
>>> print("month",month)

month 11

>>> day=now.strftime("%d")
>>> print("day",day)

day 30

time=now.strftime("%H:%M:%S")
print("time:",time)

time: 13:55:15

date_time=now.strftime("%d:%m:%y")
>>> print("datetime",date_time)

datetime 30:11:19
